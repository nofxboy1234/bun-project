{
  "name": "bun-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "bunx eslint .",
    "format": "FORCE_COLOR=1 bunx prettier . --write",
    "ts": "bunx tsc",
    "generate-routes": "bunx tsr generate",
    "watch-routes": "bunx tsr watch",
    "dev": "bun run watch-routes & bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run generate-routes && bun build --minify --sourcemap --env='BUN_PUBLIC_*' --production --target=bun ./src/index.tsx --outdir=dist",
    "start:js": "NODE_ENV=production bun --cwd=dist index.js",
    "build-exe": "bun run generate-routes && bun build --compile --minify --sourcemap --env='BUN_PUBLIC_*' --bytecode --production --target=bun ./src/index.tsx --outfile bun-project",
    "clean": "rm -rf dist && rm -f bun-project && echo 'Done cleaning.'"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@tanstack/react-router": "^1.131.44",
    "@tanstack/react-router-devtools": "^1.131.44",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tanstack/eslint-plugin-query": "^5.89.0",
    "@tanstack/eslint-plugin-router": "^1.131.2",
    "@tanstack/router-cli": "^1.131.44",
    "@types/bun": "latest",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "prettier": "3.6.2",
    "typescript-eslint": "^8.44.0"
  }
}
