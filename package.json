{
  "name": "bun-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "bunx eslint .",
    "format": "FORCE_COLOR=1 bunx prettier . --write",
    "ts": "bunx tsc",
    "dev": "bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun build --minify --sourcemap --env='BUN_PUBLIC_*' --production --target=bun ./src/index.tsx --outdir=dist",
    "build-exe": "bun build --compile --minify --sourcemap --env='BUN_PUBLIC_*' --bytecode --production --target=bun ./src/index.tsx --outfile bun-project",
    "start:js": "NODE_ENV=production bun --cwd=dist index.js",
    "clean": "rm -rf dist && rm -f bun-project && echo 'Done cleaning.'"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.87.4",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tanstack/eslint-plugin-query": "^5.86.0",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prettier": "3.6.2",
    "typescript-eslint": "^8.42.0"
  }
}
