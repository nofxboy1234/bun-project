{
  "name": "bun-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lnt": "bunx eslint .",
    "fmt": "FORCE_COLOR=1 bunx prettier . --write",
    "ts": "bunx tsc",
    "mig": "bun src/migrate.ts",
    "migd": "bun src/migrateDown.ts",
    "gen": "bun tsr generate",
    "watch": "bun tsr watch",
    "dev": "bun run watch & bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run gen && bun build --minify --sourcemap --env='BUN_PUBLIC_*' --production --target=bun ./src/index.tsx --outdir=dist",
    "start:js": "NODE_ENV=production bun --cwd=dist index.js",
    "build:exe": "bun run gen && bun build --compile --minify --sourcemap --env='BUN_PUBLIC_*' --bytecode --production --target=bun ./src/index.tsx --outfile bun-project",
    "clean": "rm -rf dist && rm -f bun-project && echo 'Done cleaning.'"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.31",
    "@tanstack/react-router-devtools": "^1.132.31",
    "@tanstack/router-cli": "^1.132.31",
    "kysely": "^0.28.7",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@postgrestools/postgrestools": "^0.15.1",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@tanstack/eslint-plugin-router": "^1.132.31",
    "@types/bun": "^1.2.23",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "3.6.2",
    "typescript-eslint": "^8.45.0"
  }
}
